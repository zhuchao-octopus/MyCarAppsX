    <style name="shadowTextCommonStyleFonts" parent="shadowText">
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>

    <style name="CommonWeightAkParamter">
        <item name="android:tag">ak_paramter=100,50</item>
        <item name="android:layout_width">0.0px</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:scaleType">center</item>
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/c_bg_button</item>
    </style>
    
        <style name="CommonWeightAkParamterVideo">
        <item name="android:tag">ak_paramter=100,50</item>
        <item name="android:layout_width">0.0px</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:scaleType">center</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="CommonStyleFonts">
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>

    <style name="CommonListViewStyle1">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:cacheColorHint">@android:color/transparent</item>
        <item name="android:descendantFocusability">blocksDescendants</item>
        <item name="android:divider">@drawable/music_splitline</item>
        <item name="android:fadingEdge">none</item>
        <item name="android:focusable">true</item>
        <item name="android:listSelector">@android:color/transparent</item>
        <item name="android:scrollbarStyle">outsideOverlay</item>
        <item name="android:scrollingCache">false</item>
    </style>

    <style name="channelButtonStyle">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">92.0px</item>
        <item name="android:layout_marginLeft">3.0px</item>
        <item name="android:textSize">40.0px</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:singleLine">true</item>
        <item name="android:paddingRight">80px</item>
        <item name="android:paddingLeft">50px</item>
        <item name="android:background">@drawable/c_bg_button</item>
    </style>
    
        <style name="channelButtonTextStyle">
        <item name="android:layout_width">58px</item>
        <item name="android:layout_height">58.0px</item>
        <item name="android:layout_marginLeft">340.0px</item>
        <item name="android:layout_marginTop">36.0px</item>
        <item name="android:textSize">26.0px</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:gravity">center</item>
    </style>
    
</resources>