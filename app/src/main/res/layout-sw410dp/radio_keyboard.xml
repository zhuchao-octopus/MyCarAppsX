<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/radio_keyboard_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
            android:focusable="false"
    android:gravity="center" >

    <RelativeLayout
        android:layout_width="495.0px"
        android:layout_height="340.0px"
        android:focusable="false"
        android:paddingLeft="0.0px"
        android:paddingTop="20.0px" >

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/radio_key_bg"
            android:focusable="false" >

            <TextView
                android:id="@+id/result"
                android:layout_width="359.0px"
                android:layout_height="59.0px"
                android:layout_marginLeft="18.0px"
                android:layout_marginTop="10.0px"
                android:background="@null"
                android:ellipsize="end"
                android:focusable="false"
                android:gravity="center|right"
                android:singleLine="true"
                android:textColor="@android:color/white"
                android:textSize="26.0px" />

            <ImageView
                android:id="@+id/num_del"
                android:layout_width="59.0px"
                android:layout_height="59.0px"
                android:layout_marginLeft="398.0px"
                android:layout_marginTop="12.0px"
                android:src="@drawable/radio_key_delete" />

            <TextView
                android:id="@+id/num1"
                android:layout_width="114.0px"
                android:layout_height="73.0px"
                android:layout_marginLeft="12.0px"
                android:layout_marginTop="78.0px"
                android:background="@drawable/radio_key1" />

            <TextView
                android:id="@+id/num2"
                android:layout_width="114.0px"
                android:layout_height="73.0px"
                android:layout_marginLeft="129.0px"
                android:layout_marginTop="78.0px"
                android:background="@drawable/radio_key2" />

            <TextView
                android:id="@+id/num3"
                android:layout_width="114.0px"
                android:layout_height="73.0px"
                android:layout_marginLeft="248.0px"
                android:layout_marginTop="78.0px"
                android:background="@drawable/radio_key3" />

            <TextView
                android:id="@+id/num0"
                android:layout_width="114.0px"
                android:layout_height="73.0px"
                android:layout_marginLeft="367.0px"
                android:layout_marginTop="78.0px"
                android:background="@drawable/radio_key0" />

            <TextView
                android:id="@+id/num4"
                android:layout_width="114.0px"
                android:layout_height="73.0px"
                android:layout_marginLeft="12.0px"
                android:layout_marginTop="156.0px"
                android:background="@drawable/radio_key4" />

            <TextView
                android:id="@+id/num5"
                android:layout_width="114.0px"
                android:layout_height="73.0px"
                android:layout_marginLeft="129.0px"
                android:layout_marginTop="156.0px"
                android:background="@drawable/radio_key5" />

            <TextView
                android:id="@+id/num6"
                android:layout_width="114.0px"
                android:layout_height="73.0px"
                android:layout_marginLeft="248.0px"
                android:layout_marginTop="156.0px"
                android:background="@drawable/radio_key6" />

            <TextView
                android:id="@+id/num_point"
                android:layout_width="114.0px"
                android:layout_height="73.0px"
                android:layout_marginLeft="367.0px"
                android:layout_marginTop="156.0px"
                android:background="@drawable/radio_key_dian" />

            <TextView
                android:id="@+id/num7"
                android:layout_width="114.0px"
                android:layout_height="73.0px"
                android:layout_marginLeft="12.0px"
                android:layout_marginTop="234.0px"
                android:background="@drawable/radio_key7" />

            <TextView
                android:id="@+id/num8"
                android:layout_width="114.0px"
                android:layout_height="73.0px"
                android:layout_marginLeft="129.0px"
                android:layout_marginTop="234.0px"
                android:background="@drawable/radio_key8" />

            <TextView
                android:id="@+id/num9"
                android:layout_width="114.0px"
                android:layout_height="73.0px"
                android:layout_marginLeft="248.0px"
                android:layout_marginTop="234.0px"
                android:background="@drawable/radio_key9" />

            <TextView
                android:id="@+id/num_ok"
                android:layout_width="114.0px"
                android:layout_height="73.0px"
                android:layout_marginLeft="367.0px"
                android:layout_marginTop="234.0px"
                android:background="@drawable/radio_key_ok" />
        </RelativeLayout>
    </RelativeLayout>

</LinearLayout>