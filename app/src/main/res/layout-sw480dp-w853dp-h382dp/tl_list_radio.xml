<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="341px"
    android:layout_height="90px"
    android:background="@drawable/radio_list_back"
    android:minHeight="90px" >

    <TextView
        android:id="@+id/list_ps_name"
        style="@style/CommonStyleFonts"
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:layout_x="20px"
        android:layout_y="20px"
        android:ellipsize="end"
        android:gravity="center"
        android:singleLine="true"
        android:textColor="#ffffffff"
        android:textSize="34.0px" />

    <TextView
        android:id="@+id/list_text1"
        style="@style/CommonStyleFonts"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_x="18px"
        android:layout_y="20px"
        android:ellipsize="end"
        android:gravity="left"
        android:singleLine="true"
        android:textColor="#ffffffff"
        android:textSize="32.0px"
        android:visibility="gone" />

    <TextView
        android:id="@+id/list_text2"
        style="@style/CommonStyleFonts"
        android:layout_width="wrap_content"
        android:layout_height="fill_parent"
        android:layout_x="220px"
        android:layout_y="26px"
        android:ellipsize="end"
        android:gravity="center_vertical"
        android:paddingLeft="1.0px"
        android:singleLine="true"
        android:textColor="#ffff0000"
        android:textSize="60.0px"/>

    <TextView
        android:id="@+id/list_text3"
        style="@style/CommonStyleFonts"
        android:layout_width="wrap_content"
        android:layout_height="0px"
        android:layout_x="280px"
        android:layout_y="20px"
        android:ellipsize="end"
        android:gravity="center_vertical"
        android:paddingLeft="1.0px"
        android:singleLine="true"
        android:textColor="#ffffffff"
        android:textSize="32.0px"
        android:visibility="gone" />

</AbsoluteLayout>