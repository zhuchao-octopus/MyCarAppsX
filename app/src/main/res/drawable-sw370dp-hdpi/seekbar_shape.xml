<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <stroke android:width="1.0px" android:color="#30ffffff" />
            <corners android:radius="5.0px" />
            <gradient android:startColor="#ff404040" android:endColor="#ff606060" android:angle="270.0" android:centerY="0.3" android:centerColor="#ff000000" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <stroke android:width="1.0px" android:color="#30ffffff" />
                <corners android:radius="5.0px" />
                <gradient android:startColor="#ff600000" android:endColor="#ff200000" android:angle="270.0" android:centerY="0.3" android:centerColor="#ffc00000" />
            </shape>
        </clip>
    </item>
</layer-list>