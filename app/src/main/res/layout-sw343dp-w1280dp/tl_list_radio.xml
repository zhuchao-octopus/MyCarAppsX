<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="426dp"
    android:layout_height="75dp"
    android:background="@drawable/stroke_black"
    android:minHeight="60dp" >

    <TextView
        android:id="@+id/list_ps_name"
        style="@style/CommonStyleFonts"
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:layout_x="20dp"
        android:layout_y="18dp"
        android:ellipsize="end"
        android:gravity="center"
        android:singleLine="true"
        android:textColor="#ffffffff"
        android:textSize="24.0dp" />

    <TextView
        android:id="@+id/list_text1"
        style="@style/CommonStyleFonts"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_x="50dp"
        android:layout_y="18dp"
        android:ellipsize="end"
        android:gravity="left"
        android:singleLine="true"
        android:textColor="#ffffffff"
        android:textSize="22.0dp" />

    <TextView
        android:id="@+id/list_text2"
        style="@style/CommonStyleFonts"
        android:layout_width="wrap_content"
        android:layout_height="fill_parent"
        android:layout_x="170dp"
        android:layout_y="10dp"
        android:ellipsize="end"
        android:gravity="center_vertical"
        android:paddingLeft="1.0dp"
        android:singleLine="true"
        android:textColor="#ffffffff"
        android:textSize="28.0dp" />

    <TextView
        android:id="@+id/list_text3"
        style="@style/CommonStyleFonts"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="340dp"
        android:layout_y="18dp"
        android:ellipsize="end"
        android:gravity="center_vertical"
        android:paddingLeft="1.0dp"
        android:singleLine="true"
        android:textColor="#ffffffff"
        android:textSize="18.0dp" />

</AbsoluteLayout>