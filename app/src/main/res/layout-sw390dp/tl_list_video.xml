<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="133.0px"
    android:layout_height="61.0px"
    android:layout_gravity="center_horizontal"
    android:minHeight="61.0px"
    android:paddingLeft="7.8px"
    android:background="@drawable/music_list_selector" >

    <!-- <ImageView -->
    <!-- android:id="@id/icon" -->
    <!-- android:layout_width="56.0px" -->
    <!-- android:layout_height="56.0px" -->
    <!-- android:layout_alignParentLeft="true" -->
    <!-- android:layout_centerVertical="true" -->
    <!-- android:visibility="gone" /> -->

    <ImageView
        android:id="@+id/play_indicator"
        android:layout_width="47.0px"
        android:layout_height="47.0px"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:layout_marginRight="6.0px"
        android:src="@drawable/play_indicator_video" />
    
	<TextView
	    android:id="@+id/list_text"
	    style="@style/CommonStyleFonts"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:layout_marginTop="17.0px"
	    android:layout_toRightOf="@+id/play_indicator"
	    android:ellipsize="end"
	    android:gravity="left"
	    android:singleLine="true"
	    android:textColor="#ffc0c0c0"
	    android:textSize="20.0px" />

</RelativeLayout>